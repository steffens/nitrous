{
  "isopack-1": {
    "name": "meteor",
    "summary": "Core Meteor environment",
    "version": "1.1.6",
    "isTest": false,
    "builds": [
      {
        "kind": "main",
        "arch": "os",
        "path": "os.json"
      },
      {
        "kind": "main",
        "arch": "web.browser",
        "path": "web.browser.json"
      },
      {
        "kind": "main",
        "arch": "web.cordova",
        "path": "web.cordova.json"
      }
    ],
    "plugins": [
      {
        "name": "basicFileTypes",
        "arch": "os",
        "path": "plugin.basicFileTypes.os/program.json"
      }
    ]
  }
}