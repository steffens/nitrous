{
  "isopack-1": {
    "name": "launch-screen",
    "summary": "Default and customizable launch screen on mobile.",
    "version": "1.0.2",
    "isTest": false,
    "builds": [
      {
        "kind": "main",
        "arch": "os",
        "path": "os.json"
      },
      {
        "kind": "main",
        "arch": "web.browser",
        "path": "web.browser.json"
      },
      {
        "kind": "main",
        "arch": "web.cordova",
        "path": "web.cordova.json"
      }
    ],
    "plugins": [],
    "cordovaDependencies": {
      "org.apache.cordova.splashscreen": "1.0.0"
    }
  }
}